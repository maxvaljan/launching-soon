{
  "cli": {
    "version": ">= 16.1.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "node": "20.0.0",
      "env": {
        "EXPO_USE_METRO_WORKSPACE_ROOT": "1"
      }
    },
    "preview": {
      "distribution": "internal",
      "node": "20.0.0",
      "env": {
        "EXPO_USE_METRO_WORKSPACE_ROOT": "1"
      }
    },
    "production": {
      "autoIncrement": true,
      "node": "20.0.0",
      "env": {
        "EXPO_USE_METRO_WORKSPACE_ROOT": "1"
      }
    }
  },
  "submit": {
    "production": {}
  }
}