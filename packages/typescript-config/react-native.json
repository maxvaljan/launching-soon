{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "React Native",
  "extends": "./base.json",
  "compilerOptions": {
    "allowJs": true,
    "jsx": "react-native",
    "lib": ["ES2019"],
    "module": "ESNext",
    "moduleResolution": "bundler",
    "noEmit": true,
    "resolveJsonModule": true,
    "target": "ES2019",
    "types": ["react-native"]
  },
  "exclude": ["node_modules"]
}